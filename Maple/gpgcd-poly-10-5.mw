<Group hide-input="false" hide-output="false" labelreference="L1" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">for k from 1 to 100 do fg := Generate(list(float(range = -10 .. 10),2*m-h+3)):
H := add(fg[i+1]*x^i, i=0..h);
Fhat := add(fg[i+1]*x^(i-h-1), i=h+1..m+1);
Ghat := add(fg[i+1]*x^(i-m-2), i=m+2..2*m-h+2);
for i from 0 to m do df[i] := Generate(float(range = -1 .. 1)) end do:
for i from 0 to m do dg[i] := Generate(float(range = -1 .. 1)) end do:
dF0 := add(df[i]*x^i, i=0..m):
dG0 := add(dg[i]*x^i, i=0..m):
dF := 0.01*dF0/norm(dF0,2):
dG := 0.01*dG0/norm(dG0,2):
Fbar := expand(H*Fhat+dF):
Gbar := expand(H*Ghat+dG):
if k=1 then F01:=Fbar; G01:=Gbar; H01:=H; FH:=Fhat; GH:=Ghat; dF01:=dF; dG01:=dG;
else F01:=F01,Fbar; G01:=G01,Gbar; H01:=H01,H; FH:=FH,Fhat; GH:=GH,Ghat; dF01:=dF01, dF; dG01:=dG01, dG; end if end do:
F01:=[F01]:G01:=[G01]:H01:=[H01]:FH01:=[FH]:GH01:=[GH]:dF01:=[dF01]:dG01:=[dG01]:</Text-field>
</Input>
</Group>
